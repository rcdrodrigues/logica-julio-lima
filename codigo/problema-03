```javascript
function calcularMedia(number1, number2, number3) {
  const somarNumeros = number1 + number2 + number3;
  const resultadoDaMedia = somarNumeros / 3;
  return resultadoDaMedia;
}

const calculoFinal1 = calcularMedia(5, 10, 25);
console.log("Valor da m√©dia dos n√∫meros inteiros √©: " + calculoFinal1);

